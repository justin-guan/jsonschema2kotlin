{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Sample",
  "type": "object",
  "definitions": {
    "test": {
      "minimum": 12,
      "maximum": 100,
      "properties": {
        "asdf": {
          "type": "string"
        }
      }
    },
    "numberEnum": {
      "enum": [1, 2, 3]
    }
  },
  "properties": {
    "nullProperty": {
      "type": "null"
    },
    "stringProperty": {
      "type": "string",
      "description": "Description",
      "enum": ["Test", "Test2", "test3"]
    },
    "obj": {
      "type": "object",
      "properties": {
        "stringProperty": {
          "type": "string",
          "enum": ["string1", "string2"]
        }
      }
    },
    "property": {
      "type": "integer",
      "$ref": "#/definitions/numberEnum"
    },
    "positiveProperty": {
      "type": "number",
      "$ref": "#/definitions/numberEnum"
    },
    "booleanProperty": {
      "type": "boolean",
      "enum": [true, false]
    },
    "arrayProperty": {
      "type": "array"
    },
    "objectProperty": {
      "type": "object",
      "properties": {
        "nestedObject": {
          "type": "object",
          "properties": {
            "nestedString": {
              "type": "string"
            },
            "nestedInteger": {
              "type": "integer"
            }
          }
        }
      }
    }
  }
}
